<Window x:Class="Занятие_в_аудитории_1_05._10._2023__Сетевое_программирование_.HttpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Занятие_в_аудитории_1_05._10._2023__Сетевое_программирование_"
        mc:Ignorable="d"
        Title="HttpWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <Button x:Name="get1Button"
            Content="GET" Click="get1Button_Click" Margin="10 5"/>
        <Button x:Name="ratesButton"
            Content="Rates" Click="ratesButton_Click" Margin="10 5"/>
        <Button x:Name="popularButton"
            Content="EUR USD XAU" Click="popularButton_Click" Margin="10 5"/>
        <Button x:Name="getXMLButton"
            Content="GET XML" Click="getXMLButton_Click" Margin="10 5"/>
        <Button x:Name="getJSONButton"
            Content="GET JSON" Click="getJSONButton_Click" Margin="10 5"/>
        <TextBlock x:Name="textBlock1" FontFamily="Courier New" Margin="10 5"/>
    </StackPanel>
</Window>
